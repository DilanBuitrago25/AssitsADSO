
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Error401</title>
    @Styles.Render("~/bundles/errors")
</head>
<body>
    <div>
        <h1>401 Error Page</h1>
        <p class="zoom-area"> No autorizado para ingresar a esta pagina o No has Iniciado Sesión</p>
        <section class="error-container">
            <span class="four"><span class="screen-reader-text">4</span></span>
